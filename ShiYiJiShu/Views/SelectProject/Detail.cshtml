@model ShiYiJiShu.Models.SelectProjectDetailModel
@{
    ViewBag.Title = "["+ Model.ClassName+"]"+ Model.SelectProject.ProjectName;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="mt15">
    <div class="left">
        @Html.Action("DoctorLeftMenu", "Doctor", new { topClassID = 7, currentClassID = Model.ClassID })
    </div>

    <div class="colmain">
        <div class="titlebg"><h2>@Model.TopClassName</h2> <span>@*<img src="~/Images/icon-home.png" />*@当前位置：首页 > @Model.TopClassName</span> </div>

        <div class="jididetail">

            <div class="jidiname">
                @Model.SelectProject.ProjectName
            </div>

            <div class="jidiinfo"> 添加时间：@Model.SelectProject.AddDateTime &nbsp; &nbsp; 阅读: @Model.SelectProject.HitCount 次  &nbsp; &nbsp; 来源：@System.Configuration.ConfigurationManager.AppSettings["WebSiteName"].ToString() </div>
 
            <div class="jidiintro">
                @Html.Raw(Model.SelectProject.ProjectIntro)
            </div>

            <div class="clear"></div>
        </div>
        </div>

    </div>
    <div class="clear"></div>
　


